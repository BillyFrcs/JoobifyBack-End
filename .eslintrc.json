{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "indent": "off",
        "object-curly-newline":"off",
        "max-len":"off",
        "comma-dangle": "off",
        "no-trailing-spaces":"off",
        "no-empty": "off",
        "brace-style": "off",
        "no-else-return": "off",
        "eol-last": "off",
        "consistent-return": "off",
        "prefer-template":"off",
        "object-property-newline": "off",
        "array-callback-return": "off",
        "prefer-destructuring": "off",
        "no-unused-expressions": "off",
        "no-unused-vars":"off",
        "quotes": "off",
        "import/no-extraneous-dependencies": "off"
    }
}